    "snapshots": {
        "depth": 50,
        "interval": 200,
        "fullPath": "snapshots/private-tangle/full_snapshot.bin",
        "deltaPath": "snapshots/private-tangle/delta_snapshot_spammer.bin",
        "deltaSizeThresholdPercentage": 50.0
